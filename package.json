{
  "name": "SOLAR_SYSTEM",
  "productName": "SOLAR_SYSTEM",
  "version": "4.0.0",
  "description": "Solar system works ",
  "main": "src/index.js",
  "icon": "src/univers.ico",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "SOLAR_SYSTEM",
    "productName": "SOLAR_SYSTEM",
    "buildVersion": "4.0",
    "copyright": "Copyright Â© 2021 AAAASTARK",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/univers.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "src/univers.ico",
      "uninstallerIcon": "src/univers.ico",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "uninstallDisplayName": "SOLAR_SYSTEM",
      "license": "src/license.txt"
    }
  },
  "keywords": [],
  "author": {
    "name": "Muhammad Allah Rakha",
    "email": "4444stark@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "SOLAR SYSTEM"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.58",
    "@electron-forge/maker-deb": "^6.0.0-beta.58",
    "@electron-forge/maker-rpm": "^6.0.0-beta.58",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.58",
    "@electron-forge/maker-zip": "^6.0.0-beta.58",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0"
  }
}
